{
  "name": "webpack-index-generator",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "reinstall": "rm -rf node_modules && npm install",
    "build:prod": "tsc -p tsconfig.prod.json",
    "build-w": "tsc -w",
    "start": "npm run build-w",
    "test-plugin": "webpack --config webpack.config.build.js --progress --colors -w",
    "build-plugin": "webpack --config ./src/webpack-index-generator/webpack.config.js --progress --colors -w",
    "deploy": "npm run build:prod && alang h m && cd dist && npm publish"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@types/chokidar": "^2.1.3",
    "@types/eslint": "^6.1.1",
    "@types/fs-extra": "^8.0.0",
    "@types/prettier": "^1.18.2",
    "@types/shelljs": "^0.8.5",
    "awesome-typescript-loader": "^5.2.1",
    "concurrently": "^4.1.0",
    "ts-loader": "^5.4.4",
    "tslint": "^5.19.0",
    "typescript": "^3.4.5",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "chokidar": "^2.1.5",
    "commander": "^2.20.0",
    "eslint": "^6.3.0",
    "fs-extra": "^8.1.0",
    "ignore": "^5.1.2",
    "prettier": "^1.18.2",
    "shelljs": "^0.8.3"
  },
  "author": "",
  "license": "MIT"
}
